{
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2022",
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "node",
    "module": "esnext",
    "outDir": "build",
    "rootDir": ".",
    "sourceMap": true,
    "lib": ["es2022", "dom", "dom.iterable"],
    "newLine":"crlf",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "actions/*": ["./src/actions/*"],
      "api": ["./src/api/*"],
      "images": ["./assets/images/*"],
      "components/*": ["./components/*"],
      "constants/*": ["./constants/*"],
      "contexts/*": ["/contexts/*"],
    }
  }
}

